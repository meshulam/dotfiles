// Place your settings in this file to overwrite the default settings
{
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "javascript.validate.enable": true,
  // Controls the delay in ms after which quick suggestions will show up
  "editor.quickSuggestionsDelay": 100,
  "editor.wordBasedSuggestions": "off",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "vim.insertModeKeyBindings": [
    {
      "before": [ "j", "k" ],
      "after": [ "<Esc>" ]
    },
    {
      "before": [ "<C-h>" ],
      "after": ["<Left>"]
    },
    {
      "before": [ "<C-l>" ],
      "after": ["<Right>"]
    }
  ],
  "vim.useSystemClipboard": true,
  "[python]": {
    "editor.rulers": [
      80,
      100
    ],
    "editor.tabSize": 4
  },
  "editor.minimap.enabled": false,
  "explorer.autoReveal": false,
  "workbench.sideBar.location": "left",
  "window.title": "${rootName}${separator}${activeEditorMedium}",
  "workbench.editor.showIcons": false,
  "files.insertFinalNewline": true,
  "editor.autoIndent": "full",
  "vim.autoindent": false,
  "editor.wordWrap": "off",
  "editor.scrollBeyondLastLine": false,
  "eslint.validate":[
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.defaultLanguage": "markdown",
  "emmet.excludeLanguages": [
    "markdown",
    "jsx",
    "tsx",
  ],
  "editor.fontSize": 14,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "github.hosts": [
    {
      "host": "https://github.com",
      "username": "oauth",
      "token": "system"
    }
  ],
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "gitlens.keymap": "alternate",
  "gitlens.advanced.messages": {
    "suppressGitVersionWarning": true,
    "suppressLineUncommittedWarning": true,
    "suppressShowKeyBindingsNotice": true
  },
  "workbench.iconTheme": "vs-minimal",
  "files.autoSave": "onFocusChange",
  "explorer.confirmDragAndDrop": false,
  "search.exclude": {
    "**/dist": true
  },
  "explorer.confirmDelete": false,
  "python.jediEnabled": false,
  "extensions.ignoreRecommendations": true,
  "go.useLanguageServer": true,
  "editor.snippetSuggestions": "none",
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.recentChange.enabled": false,
  "liveshare.comments": false,
  "liveshare.presence": true,
  "gitlens.views.lineHistory.enabled": false,
  "gitlens.views.search.enabled": false,
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": true,
      "compare": true,
      "details": false,
      "history": false,
      "remote": false
    },
    "editorTab": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "explorer": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true
    },
    "scmGroup": {
      "compare": true,
      "openClose": true,
      "stash": false,
      "stashInline": false
    },
    "scmItem": {
      "clipboard": true,
      "compare": true,
      "history": true,
      "remote": true,
      "stash": false
    }
  },
  "gitlens.hovers.currentLine.enabled": false,
  "typescript.tsserver.log": "off",
  "editor.renderControlCharacters": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "go.toolsManagement.autoUpdate": true,
  "go.alternateTools": {
    "go": "/opt/homebrew/bin/go"
  },
  "python.languageServer": "Default",
  "eslint.packageManager": "yarn",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.perEditorGroup": true,
  "csv-edit.fontSizeInPx": 14,
  "csv-edit.readOption_hasHeader": "true",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "remote.SSH.lockfilesInTmp": true,
  "remote.SSH.maxReconnectionAttempts": 3,
  "remote.SSH.remoteServerListenOnSocket": true,
  "remote.SSH.showLoginTerminal": true,
  "remote.SSH.remotePlatform": {
    "msi-vscode": "linux",
    "snow.msi.umn.edu": "linux",
    "snow-vscode": "linux",
    "gonzo": "linux",
    "radar-vscode": "linux"
  },
  "git.ignoreLegacyWarning": true,
  "gitlens.remotes": [
    {
      "domain": "github.umn.edu",
      "type": "GitHub"
    }
  ],
  "ruff.codeAction.disableRuleComment": {
    "enable": false
  },
  "files.associations": {
    "*.inc": "php",
    "*.module": "php"
  },
  "git.confirmSync": false,
  "python.analysis.autoImportCompletions": true,
  "testing.defaultGutterClickAction": "debug",
  "git.suggestSmartCommit": false,
  "[php]": {
    "files.insertFinalNewline": false,
    "editor.tabSize": 4,
  },
  "python.analysis.showOnlyDirectDependenciesInAutoImport": true,
  "python.analysis.packageIndexDepths": [

    {
      "name": "sklearn",
      "depth": 2
    },
    {
      "name": "matplotlib",
      "depth": 2
    },
    {
      "name": "scipy",
      "depth": 2
    },
    {
      "name": "django",
      "depth": 3
    },
    {
      "name": "flask",
      "depth": 2
    },
    {
      "name": "fastapi",
      "depth": 2
    }
  ],
  "redhat.telemetry.enabled": false,
  "terminal.external.osxExec": "Ghostty.app",
  "editor.fontFamily": "'Iosevka Nerd Font', Monaco, 'Courier New', monospace",
  "editor.copyWithSyntaxHighlighting": false,
  "files.trimTrailingWhitespace": true
}